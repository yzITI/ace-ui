{
  "name": "ace.svelte",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./A": {
      "types": "./dist/A/index.d.ts",
      "svelte": "./dist/A/index.js"
    },
    "./Ace": {
      "types": "./dist/Ace/index.d.ts",
      "svelte": "./dist/Ace/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "svelte": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "description": "A Progressive UI Library for Svelte 5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yzITI/ace-ui.git"
  },
  "bugs": {
    "url": "https://github.com/yzITI/ace-ui/issues"
  },
  "keywords": [
    "Svelte",
    "UI Library"
  ],
  "author": "yzITI",
  "license": "MIT",
  "peerDependencies": {
    "@mdi/js": "7.x",
    "svelte": "5.x"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^2.5.7",
    "@sveltejs/package": "^2.3.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "publint": "^0.2.7",
    "svelte": "5.0.0-next.110",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}

{
  "name": "ace.svelte",
  "version": "0.1.3",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "public": "npm publish --access public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./A": {
      "types": "./dist/A/index.d.ts",
      "svelte": "./dist/A/index.js"
    },
    "./Ace": {
      "types": "./dist/Ace/index.d.ts",
      "svelte": "./dist/Ace/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "svelte": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "description": "A Progressive UI Library for Svelte 5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yzITI/ace-ui.git"
  },
  "bugs": {
    "url": "https://github.com/yzITI/ace-ui/issues"
  },
  "keywords": [
    "Svelte",
    "UI Library"
  ],
  "author": "yzITI",
  "license": "MIT",
  "peerDependencies": {
    "@mdi/js": "7.x",
    "svelte": "^5.0.0-next.0"
  },
  "devDependencies": {
    "@mdi/js": "^7.4.47",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.7.4",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "publint": "^0.2.12",
    "svelte": "^5.1.9",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
